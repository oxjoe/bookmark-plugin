{
  "manifest_version": 2,
  "name": "ChromeBookMarkPlugin",
  "description": "DESCRIPTION",
  "version": "1.0",

  "icons": {
  "128": "icon.png"},

  "browser_action": {
    "default_title": "Example Default title"
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
//chrome.windows.* is used for opening tabs
//chrome.bookmarks.*
  "permissions": [
    "activeTab",
   "tabs",
    "https://ajax.googleapis.com/"
  ]
  "background_page": "background.html"
}