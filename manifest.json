{
  "manifest_version": 2,
  "name": "ChromeBookMarkPlugin",
  "description": "DESCRIPTION",
  "version": "999999999",

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_title": "Example Default title",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}